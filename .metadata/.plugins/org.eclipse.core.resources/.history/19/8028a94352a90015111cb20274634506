package membership.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.servlet.ModelAndView;

@Controller
public class LoginController {

	@RequestMapping("membershipLogin.action")
	public ModelAndView pageHandler(){
		System.out.println("membership.action");
		ModelAndView mav = new ModelAndView("/WEB-INF/views/membership/joinInfo.jsp");
		
		return mav;
	}
}

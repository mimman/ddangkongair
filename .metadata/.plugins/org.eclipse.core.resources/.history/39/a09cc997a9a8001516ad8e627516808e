package membership.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.servlet.ModelAndView;

@Controller
public class RegUserController {

	@RequestMapping("membership.action")
	public ModelAndView pageHandler(){
		ModelAndView mav = new ModelAndView("/WEB-INF/views/main/main.jsp");
		
		return mav;
	}
	
	@RequestMapping("membership.action")
	public ModelAndView pageHandler(){
		ModelAndView mav = new ModelAndView("/WEB-INF/views/main/if_main.jsp");
		/WEB-INF/views/main/if_main.jsp
		return mav;
	}
}
